<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
    xmlns:g='urn:import:com.google.gwt.user.client.ui'
    ui:generateFormat="com.google.gwt.i18n.server.PropertyCatalogFactory"
    ui:generateKeys="com.google.gwt.i18n.server.keygen.MethodNameKeyGenerator"
    ui:baseMessagesInterface="com.google.appinventor.client.OdeMessages"
    ui:generateLocales="default">


  <g:HTMLPanel>
    <div class="ode-propertyDialogDivHeader">
      <div class="ode-propertyDialogDivInnerHeader">
        <h3 class="ode-headerTitle">
          <ui:msg key="ProjectProperties"
                  description="Project Properties Title">Project Properties...</ui:msg>
        </h3>
      </div>
    </div>

    <g:HorizontalPanel>
      <g:VerticalPanel ui:field="panelLeftLabels"></g:VerticalPanel>
      <g:VerticalPanel ui:field="panelLeftEditors"></g:VerticalPanel>
      <g:VerticalPanel ui:field="panelRightLabels"></g:VerticalPanel>
      <g:VerticalPanel ui:field="panelRightEditors"></g:VerticalPanel>
    </g:HorizontalPanel>

  </g:HTMLPanel>

</ui:UiBinder>
